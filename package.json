{
  "name": "eth-hooks",
  "version": "2.3.2",
  "description": "A set of hooks to turbocharge buidling",
  "author": "Austin Griffith <austin@ethereum.org>",
  "repository": "https://github.com/austintgriffith/eth-hooks.git",
  "main": "index.js",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "contributors": [
    {
      "name": "Austin Griffith",
      "url": "https://github.com/austintgriffith"
    },
    {
      "name": "Tom French",
      "url": "https://github.com/tomafrench"
    },
    {
      "name": "Shravan Sunder",
      "url": "https://github.com/ShravanSunder"
    }
  ],
  "scripts": {
    "commit": "git-cz",
    "build": "ts-patch i && tsc -p tsconfig.json",
    "clean": "shx rm -rf ./lib",
    "pre-publish": "yarn clean && yarn build && shx cp -R package.json ./lib && shx cp -R README.md ./lib",
    "publish:next": "yarn pre-publish && cd lib && npm publish --tag next",
    "publish:latest": "yarn pre-publish && cd lib && npm publish --tag latest",
    "docs": "yarn pre-publish && yarn jsdoc2md ./lib/**/*.{js,jsx}",
    "deprecate": "npm deprecate",
    "lint": "eslint --config ./.eslintrc --ignore-path ./.eslintignore ./src/**/*",
    "hardhat:test": "cross-env TS_NODE_PROJECT=tsconfig.testing.json yarn hardhat test  --network hardhat",
    "hardhat:chain": "cross-env TS_NODE_PROJECT=tsconfig.testing.json yarn hardhat node"
  },
  "dependencies": {
    "@ethersproject/address": "^5.4.0",
    "@ethersproject/bignumber": "^5.4.0",
    "@ethersproject/bytes": "^5.4.0",
    "@ethersproject/contracts": "^5.4.0",
    "@ethersproject/networks": "^5.4.1",
    "@ethersproject/providers": "^5.4.1",
    "@ethersproject/signing-key": "^5.4.0",
    "@ethersproject/solidity": "^5.4.0",
    "@ethersproject/units": "^5.4.0",
    "@uniswap/token-lists": "^1.0.0-beta.24",
    "axios": "^0.21.4",
    "ethers": "^5.4.1",
    "observable-hooks": "^4.0.5",
    "use-debounce": "^7.0.0"
  },
  "peerDependencies": {
    "@uniswap/sdk": "^3.0.3",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@atixlabs/hardhat-time-n-mine": "^0.0.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.1.9",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/pretty-time": "^1.1.0",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.13",
    "@types/react-blockies": "^1.4.0",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.3",
    "@uniswap/sdk": "^3.0.3",
    "@web3-react/abstract-connector": "^6.0.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/types": "^6.0.7",
    "add": "^2.0.6",
    "chai": "^4.3.4",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.3",
    "eslint-plugin-unused-imports": "^1.1.2",
    "ethereum-waffle": "^3.4.0",
    "global-jsdom": "^8.2.0",
    "hardhat": "^2.6.4",
    "hardhat-gas-reporter": "^1.0.4",
    "http-browserify": "^1.7.0",
    "husky": "^4.2.5",
    "jsdom": "^17.0.0",
    "jsdoc-to-markdown": "^7.0.1",
    "lint-staged": "^10.2.7",
    "mocha": "^9.1.1",
    "prettier": "^2.3.1",
    "prettier-eslint": "^12.0.0",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.2.0",
    "setimmediate": "^1.0.5",
    "shx": "^0.3.3",
    "stream-browserify": "^3.0.0",
    "timers-browserify": "^2.0.12",
    "ts-node": "^10.2.1",
    "ts-patch": "^1.3.4",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.4",
    "typescript-transform-paths": "^3.0.2",
    "watch": "^1.0.2",
    "yarn": "^1.22.11"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "react",
    "create-eth-app",
    "typescript"
  ],
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
